<%= form_tag("/hub", method: "get") do %>
  <%= label_tag(:q, "Search for:") %>
  <%= text_field_tag(:q, params[:q]) %>
  <%= submit_tag("Search") %>
<% end %>
<%= render_async_cache hub_wikipedia_path(params.to_enum.to_h) do %>Wikipedia...<% end %>
<%= render_async_cache hub_claim_path(params.to_enum.to_h) do %>Google Factcheck...<% end %>
<%= render_async_cache hub_cofact_path(params.to_enum.to_h) do %>Cofacts...<% end %>
<%= render_async_cache hub_crowdtangle_path(params.to_enum.to_h) do %>CrowdTangle...<% end %>
<%= render_async_cache hub_pablo_path(params.to_enum.to_h) do %>Pablo...<% end %>